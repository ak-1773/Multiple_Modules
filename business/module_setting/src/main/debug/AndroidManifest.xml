<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <application
        android:name="com.example.setting.app.SettingApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Archives">
        <activity
            android:name="com.example.setting.activity.SettingActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Archives">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.setting.activity.SettingPowerActivity"
            android:exported="false">

        </activity>
        <activity
            android:name="com.example.setting.activity.SettingServiceActivity"
            android:exported="false">

        </activity>
        <activity
            android:name="com.example.setting.activity.SystemAboutActivity"
            android:exported="false">

        </activity>
    </application>

</manifest>